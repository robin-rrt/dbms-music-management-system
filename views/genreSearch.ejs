<!DOCTYPE html>
<html lang="en">
  <%- include('partials/head'); %> 
  <body>
    <header class="header-area">
        <%- include ('partials/navbar'); %>
      </header>
      <br>
      <br>
      

       <section class="oneMusic-buy-now-area has-fluid bg-gray section-padding-100">

        <div class="container-fluid">
            
            <div class="row">
                <div class="col-12">
                    <div class="section-heading style-2">
                        <h2>"<%= searchItem %>" <%= title %></h2>
                        
                    </div>
                </div>
            </div>

            <div class="row">

               <% for (var i = 0; i < rows.length; i++) { %>
                   <!-- Single Album Area -->
                <div class="col-12 col-sm-6 col-md-4 col-lg-2" id="<%= rows[i].title %>">
                    <a href="/song?songName=<%= rows[i].title %>">
                    <div class="single-album-area wow fadeInUp" data-wow-delay="200ms">
                        <div class="album-thumb" style="width:200px;height:200px;overflow:hidden">
                            <img src="<%= rows[i].album_uri %>" alt="<%= rows[i].album_name %>">
                        </div>
                        <div class="album-info">
                            
                                <h5><%= rows[i].title %></h5>
                                <h6><%= rows[i].artist_name %></h6>
                                <p><%= rows[i].genre_name %></p>
                        </div>
                    </div>
                    </a>
                </div>
                    <% } %>





            </div>
                </div>
            </div>
        </div>
    </section>
    <!-- ##### Buy Now Area End ##### -->
  </body>
</html>